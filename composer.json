{
    "name": "callcocam/laravel-raptor",
    "description": "This is my package laravel-raptor",
    "keywords": [
        "Claudio Campos",
        "laravel",
        "laravel-raptor"
    ],
    "homepage": "https://github.com/callcocam/laravel-raptor",
    "license": "MIT",
    "authors": [
        {
            "name": "Claudio Campos",
            "email": "contato@sigasmar.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "nunomaduro/collision": "^8.6",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Callcocam\\LaravelRaptor\\": "src/",
            "Callcocam\\LaravelRaptor\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Callcocam\\LaravelRaptor\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-raptor --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Callcocam\\LaravelRaptor\\LaravelRaptorServiceProvider"
            ],
            "aliases": {
                "LaravelRaptor": "Callcocam\\LaravelRaptor\\Facades\\LaravelRaptor"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}